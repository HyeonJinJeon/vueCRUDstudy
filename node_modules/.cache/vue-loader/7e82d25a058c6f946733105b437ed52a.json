oc(this.docId)\n          .get()\n          .then((snapshot)  => {\n            const _data = snapshot.data();\n            this.row = _data\n          })\n    },\n    modifyUser(){\n      const self = this;\n      const db = firebase.firestore();\n      db.collection(self.fbCollection)\n          .doc(this.docId)\n          .update({\n            name: self.row.name,\n            age: self.row.age,\n            gender: self.row.gender,\n            school: self.row.school,\n            schoolNim: self.row.schoolNum,\n            phoneNum: self.row.phoneNum,\n          })\n          .then(() => {\n            self.$router.push('/')\n          })\n    },\n    cancleModify(){\n      this.$router.push('/')\n    },\n    init(){\n      this.getData()\n    },\n\n  },\n  mounted() {\n    const self = this;\n    self.init();\n  },\n  components: {\n\n  }\n}\n</script>\n<style>\n.home {\n  text-align: center;\n}\n</style>\n"]}]}