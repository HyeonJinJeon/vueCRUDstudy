완료</b-button>\n    <b-button variant=\"primary\" @click=\"cancleModify\">취소</b-button>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\n\nimport {firebase} from '@/firebase/firebaseConfig';\n\nexport default {\n  name: 'home',\n  data(){\n    return {\n      fbCollection: 'users',\n      row: {},\n      docId: this.$route.params.id,\n    }\n  },\n  methods: {\n    getData(){\n      const self = this;\n      const db = firebase.firestore();\n      db.collection(self.fbCollection)\n          .doc(this.docId)\n          .get()\n          .then((snapshot)  => {\n            const _data = snapshot.data();\n            this.row = _data\n          })\n    },\n    modifyUser(){\n      const self = this;\n      const db = firebase.firestore();\n      db.collection(self.fbCollection)\n          .doc(this.docId)\n          .update({\n            name: self.row.name,\n            age: self.row.age,\n            gender: self.row.gender,\n            school: self.row.school,\n            schoolNim: self.row.schoolNum,\n            phoneNum: self.row.phoneNum,\n          })\n          .then(() => {\n            self.$router.push('/')\n          })\n    },\n    cancleModify(){\n      this.$router.push('/')\n    },\n    init(){\n      this.getData()\n    },\n\n  },\n  mounted() {\n    const self = this;\n    self.init();\n  },\n  components: {\n\n  }\n}\n</script>\n<style>\n.home {\n  text-align: center;\n}\n</style>\n"]}]}